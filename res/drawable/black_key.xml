<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<nine-patch android:src="@drawable/black_key_pressed" />
	</item>
	<item android:state_activated="true">
		<nine-patch android:src="@drawable/black_key_held" />
	</item>
	<item android:state_pressed="false">
		<nine-patch android:src="@drawable/black_key_default" />
	</item>
</selector>
